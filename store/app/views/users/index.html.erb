<h1>List User</h1>
 <table>
    <tr>
      <th>id</th>
      <th>email</th>
      <th>Name</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Birthday</th>
      <th>Gender</th>
      <th>Facebook</th>
      <th>blog</th>
      <th>Manage</th>
    </tr>
  
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.name %></td>
        <td><%= user.address %></td>
        <td><%= user.phone %></td>
        <td><%= user.birthday %></td>
        <td><%= user.gender %></td>
        <td><%= user.facebook %></td>
        <td><%= user.blog %></td>
        <td><%= link_to "Show", user_path(user.id) %> |<%= link_to "Edit", edit_user_path(user.id) %> | <%= link_to "Delete", user_path(user.id), confirm:'yakin Hapus ?',method: :delete %></td>
      </tr>
    <% end %>
  
  </table>
